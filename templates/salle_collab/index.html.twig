{% block body %}
	<body
		class="vertical-layout vertical-menu-modern  navbar-floating footer-static  " data-open="click" data-menu="vertical-menu-modern" data-col="">

		<!-- BEGIN: Content-->
		<div class="app-content content ">
			<div class="content-overlay"></div>
			<div class="header-navbar-shadow"></div>
			<div class="content-wrapper">
				<div class="content-header row"></div>
				<div class="content-body">
					<h4 class="card-title mb-50 mb-sm-0" style="margin-left:2%;margin-top:-2%;padding:1%">{{collab.getNomCollab()}}</h4>

					<div class="row match-height">
						<style>
						/* Scrollbar styles */
						::-webkit-scrollbar {
							width: 9px;
						}
						::-webkit-scrollbar-thumb {
							border-radius: 10px;
							width: 7px;
						}
						::-webkit-scrollbar-thumb:hover {
							background: #ccc;
						}
					</style>
					<!-- Employee Task Card -->
					<div class="col-lg-3 col-md-6 col-12">
						<div class="card card-employee-task">
							<div class="card-header">
								<h4 class="card-title">Liste des membres</h4>
								<i data-feather="more-vertical" class="font-medium-3 cursor-pointer"></i>
							</div>
							<div style="overflow-y : auto; height:430;background:none">
								<div class="card-body">


									{% for m in users %}
										<div class="employee-task d-flex justify-content-between align-items-center">
											<div class="media">
												<div class="avatar mr-75">
													<img src="{{m.getAvatar()}}" class="rounded" width="42" height="42" alt="Avatar"/>
												</div>
												<div class="media-body my-auto">
													<h6 class="mb-0">{{m.getPrenom()}}
														{{m.getNom()}}</h6>
													<small>iOS Developer</small>
												</div>
											</div>
											<div class="d-flex align-items-center">
												{% if collab.getIdUtilisateur() == user  %}
													{% if collab.getIdUtilisateur().getIdUtilisateur() == m.getIdUtilisateur()  %}
														<i data-feather="lock" style="width:17px; height:18px;color:green"></i>
													{% else %}

														<a href="{{ path('deleteU',{'id':m.getIdUtilisateur() ,'idu':collab.getIdCollab()}) }}">
															<i data-feather="user-minus" style="width:18px; height:18px;color:red"></i>
														</a>

													{% endif %}
												{% else %}
													👋
												{% endif  %}


											</div>
										</div>

									{% endfor %}

								</div>
							</div>
							{% if collab.getIdUtilisateur() == user  %}
								<a href="" class="btn btn-outline-primary" data-toggle="modal" data-target="#new-task-modal">Ajouter</a>
							{% else %}

								<a href="{{ path('Quitter',{'id':user.getIdUtilisateur() ,'idu':collab.getIdCollab()}) }}" class="btn btn-outline-danger">Quitter</a>


							{% endif  %}
						</div>
					</div>
					<!--/ Employee Task Card -->


					<!-- Right Sidebar starts -->
					<div class="modal modal-slide-in sidebar-todo-modal fade" id="new-task-modal">
						<div class="modal-dialog sidebar-sm">
							<div class="modal-content p-0">
