             <h4 class="card-title mb-50 mb-sm-0" style = "margin-left:2%">Collaboration </h4>
            <br><br><br>
















                         <div class="row match-height">
                                    
                                    {% for c in collabs %}                                                                   
                                                   
                  <div class="col-md-3 col-lg-3">
                            <div class="card">
                             <div class="card-header" style="max-height:20px; margin-top:-5% ;margin-bottom:3%">
                             
                                {% if c.getIdUtilisateur() == user  %}
                                   Owner
                                   {% else %}
                                    Membre
                                {% endif %} 
                                </div>
                                {%  set srs = "https://placeimg.com/64"~random(9)~"/48"~random(9)~"/tech"  %}
                                <img class="card-img-top" src="{{srs}}" style="max-height:200px;min-height:200px" alt="Card image cap" />
                                <div class="card-body">
                                    <h4 class="card-title"> {{c.getNomCollab()}} </h4>
                                    <p class="card-text">
                                        {{c.getUrlCollab()}}
                                    </p>
                                   
                                     <a href="{{ path('app_salle_collab',{'collabn':c.getNomCollab()}) }}" class="btn btn-outline-primary">Acc√©der</a>
                                     {% if c.getIdUtilisateur() == user  %}
                                  
                                   <button type="button" class="btn btn-outline-primary" data-toggle="modal" data-target="#modif" >
                                                Modifier
                                            </button>   
                                    <button type="button" class="btn btn-outline-danger" data-toggle="modal" data-target="#danger" style="margin-top:3%; min-width:105px">
                                                Effacer
                                            </button>           
                                {% endif %} 
                                    
                                    
                                </div>
                            </div>
                        </div>
 {% endfor %}


  <div class="modal fade text-left" id="danger" tabindex="-1" role="dialog" aria-labelledby="myModalLabel33" aria-hidden="true">
                                                <div class="modal-dialog modal-dialog-centered" role="document">
                                                    <div class="modal-content">
                                                        <div class="modal-header">
                                                            <h4 class="modal-title" style="color:red" id="myModalLabel33">Confirmer la suppression</h4>
